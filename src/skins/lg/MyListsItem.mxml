<?xml version="1.0" encoding="utf-8"?>
<s:ItemRenderer xmlns:fx="http://ns.adobe.com/mxml/2009" 
				xmlns:s="library://ns.adobe.com/flex/spark" 
				xmlns:mx="library://ns.adobe.com/flex/mx" 
				autoDrawBackground="true" height="20">
	<s:Group width="100%" height="100%">
		<s:Group height="100%" left="3" top="4" width="100%">
			<s:Label text="{data.green}" verticalAlign="middle" color="#67a048" fontFamily="HelveticaNeueBold" fontSize="10.95" right="3"/>
			<s:Label text="{data.black}" verticalAlign="middle" color="#6c6c6c" left="0" fontFamily="HelveticaNeueBold" fontSize="10.95" alpha="0.75"/>
		</s:Group>
		<s:Rect height="1" width="100%" bottom="0">
			<s:fill>
				<s:SolidColor alpha="0.1" color="#696969"/>
			</s:fill>
		</s:Rect>
	</s:Group>
	
</s:ItemRenderer>
