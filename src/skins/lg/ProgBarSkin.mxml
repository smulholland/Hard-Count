<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin
	xmlns:fx="http://ns.adobe.com/mxml/2009"
	xmlns:s="library://ns.adobe.com/flex/spark" 
	xmlns:mx="library://ns.adobe.com/flex/mx">
	
	<fx:Script>
		<![CDATA[
			override protected function initializationComplete():void {
				useChromeColor = true;
				super.initializationComplete();
			}
		]]>
	</fx:Script>
	
	<s:Rect radiusX="5" radiusY="5" top="0" left="0" right="0" bottom="0">
		<s:fill>
			<s:BitmapFill id="hatchingFill" source="@Embed('../../elements/hatching.png')" fillMode="repeat" />
		</s:fill>
	</s:Rect>
	
	<s:Rect radiusX="5" radiusY="5" top="0" left="0" right="0" bottom="0" alpha="0.75">
		<s:fill>
			<s:SolidColor color="0x74df1c" />
		</s:fill>
	</s:Rect>
	
</s:SparkSkin>